{
	"build-app": {
		"experimental": {
			"speed": true
		},
		"build-time-render": {
			"root": "app",
			"renderer": "jsdom",
			"writeHtml": false
		},
		"features": {
			"docs": false
		},
		"locale": "en",
		"cldrPaths": [
			"cldr-data/main/{locale}/ca-gregorian",
			"cldr-data/main/{locale}/dateFields",
			"cldr-data/main/{locale}/numbers",
			"cldr-data/main/{locale}/timeZoneNames",
			"cldr-data/supplemental/likelySubtags",
			"cldr-data/supplemental/numberingSystems",
			"cldr-data/supplemental/ordinals",
			"cldr-data/supplemental/plurals",
			"cldr-data/supplemental/timeData",
			"cldr-data/supplemental/weekData"
		],
		"externals": {
			"outputPath": ".",
			"dependencies": [
				{
					"from": "node_modules/intern/browser/intern.js",
					"to": "browser/intern.js"
				},
				{
					"from": "node_modules/intern/browser/config.js",
					"to": "browser/config.js"
				},
				{
					"from": "node_modules/intern/loaders/default.js",
					"to": "intern/loaders/default.js"
				},
				{
					"from": "node_modules/intern/index.html",
					"to": "intern/index.html"
				},
				{
					"from": "src/examples/intern.json",
					"to": "intern/intern.json"
				}
			]
		}
	},
	"build-widget": {
		"prefix": "dojo",
		"widgets": [
			"src/accordion-pane",
			"src/button",
			"src/calendar",
			"src/card",
			"src/checkbox",
			"src/checkbox-group",
			"src/chip",
			"src/combobox",
			"src/dialog",
			"src/email-input",
			"src/icon",
			"src/grid",
			"src/label",
			"src/listbox",
			"src/number-input",
			"src/outlined-button",
			"src/progress",
			"src/radio",
			"src/raised-button",
			"src/range-slider",
			"src/select",
			"src/slide-pane",
			"src/slider",
			"src/snackbar",
			"src/split-pane",
			"src/tab",
			"src/tab-controller",
			"src/text-area",
			"src/text-input",
			"src/time-picker",
			"src/title-pane",
			"src/toolbar",
			"src/tooltip"
		]
	}
}
